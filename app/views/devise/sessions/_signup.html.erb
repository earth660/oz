<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => {class: "new"}) do |f| %>
  <%= devise_error_messages! %>
  <%= f.email_field :email, :autofocus => true, id: "newmail", placeholder: "メールアドレス" %>
  <%= f.password_field :password, id: "newpassword", placeholder: "パスワード" %>
  <%= f.password_field :password_confirmation, id: "newpassword", placeholder: "パスワード(確認)" %>
  <br />
  <%= f.submit "OK", id: "new-btn" %>
  <input id="new-btn" type="button" value="OK" />
  <br />
<% end %>
